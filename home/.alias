alias addall='git add .'
alias an='archnews'
alias archnews='lynx https://archlinux.org/news/'
alias generatekey='echo "TIP: Name the key id_aneasyname."; ssh-keygen' # generate a ssh key
alias gitc='git commit'
alias gitstat='git status'
alias gp='git pull'
alias installaur='sudo pacman -U' # use pacman to install an individual package from an aur build
alias la='ls -AX' # show all directories including hidden ones and sort by extension
alias livetraffic='vnstat --live'
alias ll='ls -lAX' # show all directories and their permissions
alias makeaur='makepkg -s'
alias myconky='vim ~/.config/conky/conky.conf'
alias mydisk='sudo gnome-disks' # run Gnome Disk
alias mydrives='cd /run/media/voa' # change directory to /run/media/voa/
alias myi3='vim ~/.config/i3/config' # edit i3wm's configuration
alias myi3s='vim ~/.config/i3status/config' # Edit i3status config
alias myos='cat /etc/os-release' # show OS version
alias mypicom='vim ~/.config/picom/picom.conf' # edit picom's configuration
alias myr='ssh voa@raspberrypi' # connect to server through ssh
alias mysdf='ssh sdf.org -i $HOME/.ssh/id_sdf' # login to sdf.org account
alias myserve='python -m http.server' # run SimpleHTTPServer on current directory
alias myt='youtube-dl -f "bestvideo[ext=mp4]+bestaudio[ext=m4a]/mp4"' # download videos with mp4 video container
alias mytraffic='vnstat --query' # Show network data consumption
alias myupdate='yay -Syu' # update all ackages and AURs
alias myvim='vim $HOME/.vimrc'
alias mywallpaper='feh --bg-tile /home/voa/Pictures/patterns/cubes.xpm.gif'
alias restartnetworkmanager='sudo systemctl restart NetworkManager'
alias testdunst='notify-send "Title" "NOW"' # test dunst's notifications
